{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": ["build", "${workspaceFolder}/Project.sln"],
            "problemMatcher": "$msCompile",
            "group": { "kind": "build", "isDefault": true }
        },
        {
            "label": "build-api",
            "command": "dotnet",
            "type": "process",
            "args": ["build", "${workspaceFolder}/Project.Api/Project.Api.csproj"],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-web",
            "command": "dotnet",
            "type": "process",
            "args": ["build", "${workspaceFolder}/Project.Web/Project.Web.csproj"],
            "problemMatcher": "$msCompile"
        }
    ]
}
